            </trans-unit>
            <trans-unit id="vehicle.purchase_date.in_future">
                <source>vehicle.purchase_date.in_future</source>
                <target>La date ne peut pas être dans le futur</target>
            </trans-unit>
            <trans-unit id="vehicle.purchase_date.not_blank">
                <source>vehicle.purchase_date.not_blank</source>
                <target>La date ne peut pas être vide</target>
            </trans-unit>
