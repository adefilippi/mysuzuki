        <body>
            <!-- Validation de l'utilisateur -->
            <trans-unit id="user.unique">
                <source>user.unique</source>
                <target>Un utilisateur de ce nom existe déjà pour ce véhicule</target>
            </trans-unit>
            <trans-unit id="user.email.unique">
                <source>user.email.unique</source>
                <target>Cet email est déjà utilisé</target>
            </trans-unit>
            <trans-unit id="user.last_name.not_blank">
                <source>user.last_name.not_blank</source>
                <target>Votre nom de famille est obligatoire</target>
            </trans-unit>
            <trans-unit id="user.first_name.length">
                <source>user.first_name.max_length</source>
                <target>Votre prénom ne peut pas faire plus de 32 charactères</target>
            </trans-unit>
            <trans-unit id="user.password.not_blank">
                <source>user.password.not_blank</source>
                <target>Le mot de passe est obligatoire</target>
            </trans-unit>
            <trans-unit id="user.email.is_email">
                <source>user.email.is_email</source>
                <target>Votre email ne respecte pas le bon format</target>
            </trans-unit>
            <trans-unit id="user.email.not_blank">
                <source>user.email.not_blank</source>
                <target>Votre email ne peut pas être vide</target>
            </trans-unit>
            <trans-unit id="user.email.max_lenth">
                <source>user.email.max_lenth</source>
                <target>Votre email est trop long</target>
            </trans-unit>
            <trans-unit id="user.optin.true">
                <source>user.optin.true</source>
                <target>Vous devez accepter l'utilisation de votre email par Suzuki France</target>
            </trans-unit>
            <trans-unit id="user.job.name.max_length">
                <source>user.job.name.max_length</source>
                <target>L'intitulé de votre poste est trop long</target>
            </trans-unit>
            <trans-unit id="user.job.department.max_length">
                <source>user.job.department.max_length</source>
                <target>Le nom de votre service est trop long</target>
            </trans-unit>
            <trans-unit id="user.organization.name.max_length">
                <source>user.organization.name.max_length</source>
                <target>Le raison sociale es trop longue</target>
            </trans-unit>
            <trans-unit id="user.organization.number_vehicles.max_length">
                <source>user.organization.number_vehicles.max_length</source>
                <target>Le nombre de véhicules est trop grand</target>
            </trans-unit>
