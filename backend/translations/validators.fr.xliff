                <target>L'email ne respecte pas le bon format</target>
            </trans-unit>
            <trans-unit id="password_request_token.new_password.not_blank">
                <source>password_request_token.new_password.not_blank</source>
                <target>Le mot de passe ne doit pas être vide</target>
            </trans-unit>

            <!-- Validation des demandes formulaire de contact -->
            <trans-unit id="issue.email.not_null_no_user">
                <source>issue.email.not_null_no_user</source>
                <target>L'email doit être spécifié si vous n'êtes pas connecté</target>
            </trans-unit>
            <trans-unit id="issue.type.not_null">
                <source>issue.type.not_null</source>
                <target>Le type doit être spécifié</target>
            </trans-unit>
            <trans-unit id="issue.subject.not_blank">
                <source>issue.subject.not_blank</source>
                <target>Le type doit être spécifié</target>
            </trans-unit>
            <trans-unit id="issue.message.not_blank">
                <source>issue.message.not_blank</source>
