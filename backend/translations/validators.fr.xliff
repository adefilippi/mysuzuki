                <source>user.organization.siret.max_length</source>
                <target>Le numéro de SIRET est trop long</target>
            </trans-unit>
            <trans-unit id="user.date_of_birth.format">
                <source>user.date_of_birth.format</source>
                <target>Votre date de naissance ne respecte pas le bon format</target>
            </trans-unit>
            <trans-unit id="user.address.street.max_length">
                <source>user.address.street.max_length</source>
                <target>Votre nom de rue est trop long</target>
            </trans-unit>
            <trans-unit id="user.address.street.not_blank">
                <source>user.address.street.not_blank</source>
                <target>Votre nom de rue ne peut pas être vide</target>
            </trans-unit>
            <trans-unit id="user.address.additional1.max_length">
                <source>user.address.additional1.max_length</source>
                <target>Votre complément d'adresse est trop long</target>
            </trans-unit>
            <trans-unit id="user.address.additional2.max_length">
                <source>user.address.additional1.max_length</source>
                <target>Votre complément d'adresse est trop long</target>
            </trans-unit>
            <trans-unit id="user.address.zip_code.max_length">
                <source>user.address.zip_code.max_length</source>
                <target>Votre code postal doit faire 5 caractères maximum</target>
            </trans-unit>
            <trans-unit id="user.address.zip_code.not_blank">
                <source>user.address.zip_code.not_blank</source>
                <target>Votre code postal ne peut pas être vide</target>
            </trans-unit>
            <trans-unit id="user.address.city.max_length">
                <source>user.address.city.max_length</source>
                <target>Votre nom de ville est trop long</target>
            </trans-unit>
            <trans-unit id="user.address.city.not_blank">
                <source>user.address.city.not_blank</source>
                <target>Votre nom de ville ne peut pas être vide</target>
            </trans-unit>
            <trans-unit id="user.civ.choice">
                <source>user.civ.choice</source>
                <target>Vous devez choir M ou MME</target>
            </trans-unit>
            <trans-unit id="user.mobile_phone.format">
                <source>user.mobile_phone.format</source>
                <target>Votre numéro mobile ne respecte pas le bon format</target>
            </trans-unit>
            <trans-unit id="user.landline_phone.format">
                <source>user.landline_phone.format</source>
                <target>Votre numéro fixe ne respecte pas le bon format</target>
            </trans-unit>
            <trans-unit id="user.phone.one_or_both">
                <source>user.phone.one_or_both</source>
                <target>Vous devez renseigner au moins un des deux numéros</target>
            </trans-unit>


            <!-- Validation des opérations véhicules-->
            <trans-unit id="maintenance.date.format">
                <source>maintenance.date.format</source>
                <target>La date de l'opération ne respecte pas le bon format</target>
            </trans-unit>
            <trans-unit id="maintenance.type.not_blank">
                <source>maintenance.type.not_blank</source>
                <target>Le type de l'opération est obligatoire</target>
            </trans-unit>
            <trans-unit id="maintenance.place.not_blank">
                <source>maintenance.place.not_blank</source>
                <target>Le lieu de l'opération est obligatoire</target>
            </trans-unit>

            <!-- Validation du changement de mot de passe -->
            <trans-unit id="password_change.old_password.not_blank">
                <source>password_change.old_password.not_blank</source>
                <target>L'ancien mot de passe est obligatoire</target>
            </trans-unit>
            <trans-unit id="password_change.old_password.match">
                <source>password_change.old_password.match</source>
                <target>L'ancien mot de passe est incorrect</target>
            </trans-unit>
            <trans-unit id="password_change.new_password.not_blank">
                <source>password_change.new_password.not_blank</source>
                <target>Le nouveau mot de passe est obligatoire</target>
            </trans-unit>
            <trans-unit id="password_change.new_password.same">
                <source>password_change.new_password.same</source>
                <target>L'ancien et le nouveau mot de passe ne doivent pas être identiques</target>
            </trans-unit>

            <!-- Validation du mot de passe oublié -->
            <trans-unit id="password_request_token.email.not_blank">
                <source>password_request_token.email.not_blank</source>
