            <trans-unit id="offer.media.not_null">
                <source>offer.media.not_null</source>
                <target>Vous devez obligatoirement fournir une image</target>
            </trans-unit>

            <trans-unit id="offer.game.unique">
                <source>offer.game.unique</source>
                <target>Vous ne pouvez participer qu'une seule fois à un jeu.</target>
            </trans-unit>

            <trans-unit id="offer.game.opened">
                <source>offer.game.opened</source>
                <target>Ce jeu est terminé.</target>
            </trans-unit>
            <trans-unit id="offer.game.full">
