            </trans-unit>

            <!-- Validation des pièces jointes du formulaire contact -->
            <trans-unit id="attachment.file.not_null">
                <source>attachment.file.not_null</source>
                <target>Le fichier ne peut pas être vide</target>
            </trans-unit>
            <trans-unit id="attachment.file.max_size">
                <source>attachment.file.max_size</source>
                <target>Le fichier doit faire au maximum 10Mo</target>
            </trans-unit>

            <!-- Validation des tokens -->
            <trans-unit id="token.url.not_blank">
                <source>token.url.not_blank</source>
                <target>L'URL est nécessaire pour pouvoir construire le lien.</target>
            </trans-unit>

